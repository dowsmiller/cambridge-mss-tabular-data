               <msContents>
                  <summary>
                     <seg part="N" type="para">This fragment, probably written in the 16th century, is part of a sheet of paper bearing notes in Latin on the mutation of Greek letters.</seg>
                     <seg part="N" type="para">Dr Christopher Wright</seg>
                  </summary>
                  <textLang mainLang="la" otherLangs="grc">Latin and Greek</textLang>
                  <msItem n="1">
                     <locus from="1r" to="1r">Recto</locus>
                     <title xml:lang="en">
                                    Notes on Greek letters</title>
                     <note anchored="true">Defective at the beginning and end</note>
                     <incipit defective="true">
                        <locus from="1r" to="1r">Recto</locus> ]n quibus mutat</incipit>
                     <explicit defective="true">
                        <locus from="1r" to="1r">Recto</locus> πληθω</explicit>
                     <note anchored="true">The <locus from="1v" to="1v">verso</locus> is blank</note>
                  </msItem>
               </msContents>
