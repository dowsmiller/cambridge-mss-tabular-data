                                <extent>
                                    <seg type="object">
                              <objectType>Codex </objectType>
                           </seg>
                                    <seg type="leaf">
                                        <measure quantity="2" type="laterEndleaf">ii</measure> + <measure quantity="16" type="leaf">16</measure> + <measure quantity="14" type="laterEndleaf">xiv</measure>
                                    </seg>, <seg type="dimensions">
