                    <msContents>
                        <summary>
                            <seg type="para">This fragment is a scrap of parchment containing part
                                of the Gospel of Mark, in a script perhaps dating to the 7th
                                century.</seg>
                            <seg type="para">Dr Christopher Wright</seg>
                        </summary>

                        <textLang mainLang="grc">Greek</textLang>

                        <msItem n="1">
                            <title xml:lang="eng">Gospel of Mark</title>
                            <note>This fragment contains parts of Mk 14:52-53 and Mk 14:61-62</note>
                            <note>Gregory-Aland 0317</note>
                            <incipit defective="true">
                                <locus from="[1] recto" to="[1] recto">Recto </locus>
                                <!-- <supplied reason="damage">εφυ</supplied> -->γεν και
                                    απηγαγο<supplied reason="damage">ν</supplied>
                     </incipit>
                            <explicit defective="true">
                                <locus from="[1] verso" to="[1] verso">Verso</locus> εκ
                                δεξιων καθημενον <supplied reason="damage">τ</supplied>ης
                                    δυναμεω<supplied reason="damage">ς</supplied> κα<supplied reason="damage">ι</supplied>
                                ερχο<!-- <supplied reason="damage">μενον</supplied> --></explicit>
                            <note>TLG 0031.002</note>
                        </msItem>
                    </msContents>

